{
    "family": "signalfx-fargate",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "entryPoint": [
                "bash",
                "-c"
            ],
            "portMappings": [],
            "command": [
                "curl --fail $CONFIG_URL > /etc/signalfx/agent.yaml && exec /bin/signalfx-agent"
            ],
            "environment": [
                {
                    "name": "ACCESS_TOKEN",
                    "value": "YOURTOKENHERE"
                },
                {
                    "name": "INGEST_URL",
                    "value": "https://ingest.YOURREALMHERE.signalfx.com"
                },
                {
                    "name": "API_URL",
                    "value": "https://api.YOURREALMHERE.signalfx.com"
                },
                {
                    "name": "CONFIG_URL",
                    "value": "https://raw.githubusercontent.com/slernersplunk/agent/master/fargate/agent.yaml"
                }
            ],
            "dockerLabels": {
                "app": "signalfx-agent"
            },
            "name": "signalfx-agent",
            "image": "quay.io/signalfx/signalfx-agent:5.3.2"
        }
    ],
    "cpu": "256",
    "memory": "512",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "placementConstraints": []
}
